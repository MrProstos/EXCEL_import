{% extends 'base.twig' %}
{% block content %}
    <div class="api box block">
        {% include 'navbar.twig' %}
        <div class="api-get level">
            <input class="button api-get__button level-left" type="button" value="Получить токен" onclick="getToken()">
            <span class="token level-item"></span>
        </div>
        <div class="method-add block">
            <button class="method-add__button button" onclick="Add()">Отправить add запрос</button>
        </div>
        <div class="method-add block">
            <button class="method-add__button button" onclick="Update()">Отправить update запрос</button>
        </div>
        <div class="method-add block">
            <button class="method-add__button button" onclick="Get()">Отправить get запрос</button>
        </div>
        <div class="method-add block">
            <button class="method-add__button button" onclick="Delete()">Отправить delete запрос</button>
        </div>
        <div class="method-add block">
            <button class="method-add__button button" onclick="randomDataSend()">Отправить случайные данные</button>
        </div>
    </div>
{% endblock %}